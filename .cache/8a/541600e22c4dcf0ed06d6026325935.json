: 30px 30px;\r\n  text-align: center;\r\n  padding: 40px; /* Add padding to create space around the text */\r\n  background-color: #cf087c; /* Bratz Pink background for the section */\r\n  color: #FFF; /* White text color */\r\n  border-radius: 10px;\r\n  max-width: 75%;\r\n  width: 100%;\r\n  \r\n}\r\n\r\n.bratzHeader {\r\n  font-family: 'BratzFont', Arial, sans-serif; /* Thinking of using a custom font, otherwise I'm going to fallback on Arial or sans-serif */\r\n  font-size: 40px; \r\n  color: purple; \r\n  text-transform: uppercase; /* Making the text uppercase like the logo */\r\n  letter-spacing: 3px; /* May increase letter spacing for a more stylized look */\r\n  text-shadow: 2px 2px 4px rgba(210, 213, 122, 0.955); /* Addint a subtle text shadow for depth */         \r\n}\r\n\r\n.draggable {\r\n  width: 300px;\r\n  height: 200px;\r\n  background-color: #02f503; /* Purple background */\r\n  color: #FFF; /* White text color */\r\n  text-align: center;\r\n  cursor: grab; /* Change cursor type */\r\n  position: absolute;\r\n  border-radius: 10px;\r\n  box-shadow: 0px 5px 15px rgba(0, 0, 0, 0.3); /* Add a shadow */\r\n}\r\n\r\n#draggable-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: flex-end;\r\n  height: 100%;\r\n}\r\n\r\n\r\n.close-button {\r\n  position: absolute;\r\n  top: 10px;\r\n  right: 10px;\r\n  background: none;\r\n  border: none;\r\n  color: #FFF;\r\n  font-size: 18px;\r\n  cursor: pointer;\r\n}\r\n\r\n.cd-button {\r\n  width: 110px;\r\n  height: 150px;\r\n  background-color: #8B008B; /* Purple */\r\n  border-radius: 50%;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  cursor: pointer;\r\n  position:static;\r\n  bottom: 20px;\r\n  right: 20px;\r\n  color: #FFF;\r\n  font-weight: bold;\r\n  font-size: 14px;  \r\n  padding: 10px 20px;\r\n  \r\n}\r\n\r\n/* Change Background Button */\r\n#change-bg-button {\r\n  background-color: #8B008B; /* Purple */\r\n  color: #FFF;\r\n  padding: 10px 20px;\r\n  border: none;\r\n  cursor: pointer;\r\n  border-radius: 5px;\r\n  margin: 20px;\r\n}\r\n\r\n.cd-inner {\r\n  transform: rotate(45deg); /* Rotate the inner content to create the CD effect */\r\n}\r\n\r\n.popup-content {\r\n  padding: 30px;\r\n  padding-top: 50px; /* Adjust content padding to make space for the close button */\r\n}\r\n\r\n.popup-content h2 {\r\n  margin: 0;\r\n  font-size: 24px;\r\n}\r\n\r\n.popup-content p {\r\n  margin-top: 10px;\r\n  font-size: 18px;\r\n}\r\n\r\n.menu-item {\r\n  color: #FFF;\r\n  margin: 0 10px;\r\n  cursor: pointer;\r\n}\r\n\r\n.dropdown {\r\n  float: left;\r\n  overflow: hidden;\r\n  padding: 10px 10px;\r\n}\r\n\r\n.dropdown-content {\r\n  display: none;\r\n  position: absolute;\r\n  background-color: #0387b1;\r\n  min-width: 160px;\r\n  box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);\r\n  z-index: 1;\r\n}\r\n\r\n.dropdown:hover .dropdown-content\r\n{\r\n  display: block;\r\n}\r\n\r\n.dropdown ul {\r\n  list-style: none; /* Remove default list bullets */\r\n  padding: 0;\r\n  margin: 0;\r\n}\r\n\r\n.dropdown ul li {\r\n  margin: 5px 0; /* Add spacing between list items */\r\n}\r\n\r\n.dropdown ul li a {\r\n  display: block; /* Make the entire list item clickable */\r\n  text-decoration: none;\r\n  color: #FFF;\r\n  padding: 5px 0; /* Add padding to make it visually appealing */\r\n  text-align: left;\r\n}\r\n\r\n.dropdown ul li a:hover {\r\n  background-color: purple; /* Highlight on hover */\r\n}\r\n\r\n#menu-bar {\r\n  width: 80%;\r\n  background-color: #0387b1;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  padding: 15px 0;\r\n  border-radius: 30px;\r\n  margin: 20px auto; /* Add margin for spacing around the menu-bar and center it horizontally */\r\n}\r\n\r\n\r\n#footer {\r\n  width: 100%;\r\n  color: #FFF;\r\n  text-align: center;\r\n  padding: 10px 0;\r\n}\r\n\r\n/* Desktop Icons */\r\n.desktop-icon {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  margin: 20px 0;\r\n}\r\n\r\n.desktop-icon a {\r\n  text-decoration: none;\r\n  color: #8B008B; /* Purple */\r\n  font-weight: bold;\r\n  margin-top: 5px;\r\n}\r\n\r\n.desktop-icon img {\r\n  width: 100%;\r\n  height: 500px;\r\n  margin-bottom: 10px;\r\n  border-radius: 10px;\r\n}\r\n\r\n.container {\r\n  border-radius: 5px;\r\n  background-color: #f2f2f2;\r\n  padding: 20px;\r\n}\r\n\r\n.popup {\r\n  position: relative;\r\n  display: inline-block;\r\n  cursor: pointer;\r\n  -webkit-user-select: none;\r\n  -moz-user-select: none;\r\n  -ms-user-select: none;\r\n  user-select: none;\r\n}\r\n\r\n/* The actual popup */\r\n.popup .popuptext {\r\n  visibility: hidden;\r\n  width: 250px;\r\n  height: 250px;\r\n  background-color: teal;\r\n  color: #fff;\r\n  text-align: center;\r\n  border-radius: 6px;\r\n  padding: 8px 0;\r\n  position: absolute;\r\n  z-index: 1;\r\n  top: 40px;\r\n  left: 50%;\r\n  margin-left: -80px;\r\n}\r\n\r\n/* Toggle this class - hide and show the popup */\r\n.popup .show {\r\n  visibility: visible;\r\n  -webkit-animation: fadeIn 1s;\r\n  animation: fadeIn 1s;\r\n}"},"lineCount":306}},"error":null,"hash":"3fe31fc085b303e86f45e941d3f47ffd","cacheData":{"env":{}}}